{
  "name": "openfeelz",
  "version": "1.0.0",
  "description": "PAD + Ekman + OCEAN emotional model plugin for OpenClaw agents",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./mcp": "./dist/src/mcp/mcp-server.js"
  },
  "bin": {
    "openfeelz-mcp": "./dist/src/mcp/mcp-server.js"
  },
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "build": "tsc -p tsconfig.build.json",
    "typecheck": "tsc --noEmit",
    "lint": "npx oxlint ."
  },
  "openclaw": {
    "extensions": [
      "./dist/index.js"
    ]
  },
  "peerDependencies": {
    "openclaw": ">=2026.1.0"
  },
  "dependencies": {
    "@clack/prompts": "^1.0.0",
    "@modelcontextprotocol/sdk": "^1.26.0",
    "commander": "^14.0.3"
  },
  "devDependencies": {
    "@sinclair/typebox": "^0.34.48",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/trianglegrrl/openfeelz.git"
  },
  "homepage": "https://github.com/trianglegrrl/openfeelz#readme",
  "bugs": {
    "url": "https://github.com/trianglegrrl/openfeelz/issues"
  },
  "keywords": [
    "openclaw",
    "plugin",
    "emotion",
    "affective-computing",
    "personality",
    "ocean",
    "pad",
    "ai-agent",
    "mcp"
  ],
  "engines": {
    "node": ">=22"
  },
  "license": "MIT",
  "files": ["dist", "LICENSE", "README.md", "openclaw.plugin.json"],
  "prepublishOnly": "npm run build"
}